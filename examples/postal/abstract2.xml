int[0,NV] tally_1;
int[0,NV] tally_2;
int[0,NV] tally_3;


// Auxiliary
int tallySum(){
  return tally_1+tally_2+tally_3;
}

int packSum(){
  int i,res;
  for(i=0;i&lt;=NV;i++){
    res = res + pack_sent[i];
  }
  return res;
